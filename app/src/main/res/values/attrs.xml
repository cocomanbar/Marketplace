<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- TypefaceTextView字体-->
    <declare-styleable name="TypefaceTextView">
        <attr name="typeface">
            <enum name="fzlLTypeface" value="1" />
            <enum name="fzdb1Typeface" value="2" />
            <enum name="futuraTypeface" value="3" />
            <enum name="dinTypeface" value="4" />
            <enum name="lobsterTypeface" value="5" />
        </attr>
    </declare-styleable>

    <!-- TabItemView属性-->
    <declare-styleable name="TabItemView">
        <attr name="isSelected" format="boolean" />
        <attr name="normal_text" format="string" />
        <attr name="high_text" format="string" />
        <attr name="normal_src" format="reference|color" />
        <attr name="high_src" format="reference|color" />
    </declare-styleable>

</resources>